{
  "name": "electron-vite-starter",
  "type": "commonjs",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.2.0",
  "description": "__TODO__",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue",
    "vue3"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "ntnyq/electron-vite-starter",
  "main": "./out/main/index.js",
  "scripts": {
    "build": "npm run typecheck && electron-vite build",
    "build:linux": "npm run build && electron-builder --linux",
    "build:mac": "npm run build && electron-builder --mac",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "dev": "electron-vite dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky",
    "start": "electron-vite preview",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false"
  },
  "dependencies": {
    "@iconify/json": "^2.2.302",
    "@ntnyq/utils": "^0.4.5",
    "@unocss/reset": "^65.4.3",
    "@vueuse/core": "^12.5.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "floating-vue": "^5.2.2",
    "pinia": "^2.3.1",
    "unplugin-vue-router": "^0.11.2",
    "vue-i18n": "^11.1.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@ntnyq/eslint-config": "^4.0.0-beta.1",
    "@ntnyq/prettier-config": "^1.22.1",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-updater": "^6.3.9",
    "electron-vite": "^2.3.0",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "is-path-inside": "^4.0.0",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "tinyexec": "^0.3.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.11",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json,svg}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
